{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "types": "templates/component/types.ts",
        "index": "templates/component/index.ts",
        "style": "templates/component/styles.ts",
        "test": "templates/component/TemplateName.test.tsx",
        "story": "templates/component/TemplateName.stories.tsx"
      },
      "path": "src/components/atoms",
      "withTypes": true,
      "withIndex": true,
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false
    }
  }
}